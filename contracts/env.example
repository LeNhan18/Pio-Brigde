# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Validator addresses (comma-separated, exactly 5 addresses)
VALIDATORS=0x1234567890123456789012345678901234567890,0x2345678901234567890123456789012345678901,0x3456789012345678901234567890123456789012,0x4567890123456789012345678901234567890123,0x5678901234567890123456789012345678901234

# PIO Token address on Pione Zero (for PIOLock deployment)
PIO_TOKEN=0x0000000000000000000000000000000000000000

# Optional: Enable contract verification
VERIFY=false

# RPC URLs (optional, will use default if not set)
PIONEZERO_RPC_URL=https://rpc.zeroscan.org
GOERLI_RPC_URL=https://rpc.ankr.com/eth_goerli